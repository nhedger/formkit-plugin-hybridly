{
  "name": "@hedger/formkit-plugin-hybridly",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@8.1.1",
  "description": "FormKit plugin for Hybridly.",
  "author": {
    "name": "Nicolas Hedger",
    "email": "nicolas@hedger.ch"
  },
  "license": "MIT",
  "homepage": "https://github.com/nhedger/formkit-plugin-hybridly#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nhedger/formkit-plugin-hybridly.git"
  },
  "bugs": "https://github.com/nhedger/formkit-plugin-hybridly/issues",
  "keywords": [
    "formkit",
    "hybridly"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub"
  },
  "devDependencies": {
    "@formkit/core": "^0.16.4",
    "@hedger/prettier-config": "^1.2.0",
    "@types/node": "^18.15.11",
    "hybridly": "^0.2.1",
    "pnpm": "^8.1.1",
    "prettier": "^2.8.7",
    "typescript": "^5.0.4",
    "unbuild": "^1.2.0",
    "vitest": "^0.29.8"
  },
  "peerDependencies": {
    "@formkit/core": "^0.16.4",
    "hybridly": "^0.2.1"
  }
}
